<%= form_with url: subscription_path, local: true, id: "payment-form" do |f| %>
    <%= f.collection_select :plan, Plan.all, :processor_id, :formatted_name %>
    <div id="card-element" class="MyCardElement"></div>
    <div id="card-errors" role="alert"></div>
    <%= f.label :name_on_card %>
    <%= f.text_field :name_on_card %>
    <%= f.submit %>
<% end %>