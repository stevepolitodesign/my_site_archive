<%= form_with model: [@website, @webpage], local: true do |f| %>
    <%= render partial: "shared/form_errors", locals: { object: f.object } %>
    <%= f.label :title %>
    <%= f.text_field :title, required: true %>
    <%= f.label :url %>
    <%= f.url_field :url, required: true, value: @website.url %>
    <%= f.submit %>
<% end %>